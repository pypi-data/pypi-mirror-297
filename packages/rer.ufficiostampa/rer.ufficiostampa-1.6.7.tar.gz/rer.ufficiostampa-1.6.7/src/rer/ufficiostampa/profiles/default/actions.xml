<?xml version="1.0" encoding="utf-8"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object" purge="False" >
  <object name="channels-management" meta_type="CMF Action"
     i18n:domain="rer.ufficiostampa">
   <property name="title" i18n:translate="">Channels management</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/@@channels-management</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="permissions">
    <element value="rer.ufficiostampa: Manage Channels"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="history-management" meta_type="CMF Action"
     i18n:domain="rer.ufficiostampa">
   <property name="title" i18n:translate="">History management</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/@@history-management</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="permissions">
    <element value="rer.ufficiostampa: Access History"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
