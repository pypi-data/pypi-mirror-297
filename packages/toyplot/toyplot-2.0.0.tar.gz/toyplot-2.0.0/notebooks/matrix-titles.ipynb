    "import numpy\n",
    "import toyplot\n",
    "\n",
    "data = numpy.random.normal(size=(4,4))\n",
    "\n",
    "canvas, table = toyplot.matrix(data)\n",
    "for i, row in enumerate(data):\n",
    "    for j, value in enumerate(row):\n",
    "        table.body.cell[i,j].title = f\"({i},{j}) {value:.3f}\""
